<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20240513/MODEL/"
             xmlns:semantic="https://www.omg.org/spec/DMN/20240513/MODEL/"
             xmlns:dmn11="https://www.omg.org/spec/DMN/20230324/DMN15.xsd"
             xmlns:rss="http://purl.org/rss/2.0/"
             xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/"
             xmlns:drools="http://www.drools.org/kie/dmn/1.1"
             xmlns:trisofeed="http://trisotech.com/feed"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20230324/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/"
             xmlns:triso="http://www.trisotech.com/2015/triso/modeling"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns:feel="https://www.omg.org/spec/DMN/20240513/FEEL/"
             xmlns:include1="http://www.trisotech.com/definitions/_ae5b3c17-1ac3-4e1d-b4f9-2cf861aec6d9"
             xmlns:trisodmn="http://www.trisotech.com/2016/triso/dmn"
             xmlns:tc="http://www.omg.org/spec/DMN/20160719/testcase"
             id="feel-model"
             name="FEEL Model"
             namespace="http://www.trisotech.com/definitions/feelModel"
             expressionLanguage="https://www.omg.org/spec/DMN/20240513/FEEL/">

            <!--built-in functions returning a Number-->
            <decision id="decision1" name="sum">
                <variable name="sumResult" typeRef="number"/>
                <literalExpression>
                    <text>sum([1, null, 3])</text>
                </literalExpression>
            </decision>

            <decision id="decision2" name="mean">
                <variable name="meanResult" typeRef="number"/>
                <literalExpression>
                    <text>mean([1, "a", 3])</text>
                </literalExpression>
            </decision>

            <!--built-in functions returning a String-->
            <decision id="decision3" name="lowercase">
                <variable name="lowercaseResult" typeRef="string"/>
                <literalExpression>
                    <text>lower case(12)</text>
                </literalExpression>
            </decision>

            <!--built-in functions returning a Boolean-->
            <decision id="decision4" name="checkBoolean">
                <variable name="booleanResult" typeRef="boolean"/>
                <literalExpression>
                    <text>"a" > 1</text>
                </literalExpression>
            </decision>
</definitions>
