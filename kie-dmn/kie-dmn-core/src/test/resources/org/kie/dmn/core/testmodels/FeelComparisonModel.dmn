<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20230324/MODEL/"
             xmlns:semantic="https://www.omg.org/spec/DMN/20230324/MODEL/"
             xmlns:dmn11="https://www.omg.org/spec/DMN/20230324/DMN15.xsd"
             xmlns:rss="http://purl.org/rss/2.0/"
             xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/"
             xmlns:drools="http://www.drools.org/kie/dmn/1.1"
             xmlns:trisofeed="http://trisotech.com/feed"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20230324/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/"
             xmlns:triso="http://www.trisotech.com/2015/triso/modeling"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns:feel="https://www.omg.org/spec/DMN/20230324  /FEEL/"
             xmlns:include1="http://www.trisotech.com/definitions/_ae5b3c17-1ac3-4e1d-b4f9-2cf861aec6d9"
             xmlns:trisodmn="http://www.trisotech.com/2016/triso/dmn"
             xmlns:tc="http://www.omg.org/spec/DMN/20160719/testcase"
             id="feel-comparison-model"
             name="FEEL ComparisonModel"
             namespace="http://www.trisotech.com/definitions/feelComparisonModel"
             expressionLanguage="https://www.omg.org/spec/DMN/20230324/FEEL/">

            <!-- Input: inputA (string) -->
            <inputData id="input1" name="inputA">
                <variable name="inputA" typeRef="string"/>
            </inputData>

            <!-- Input: inputB (number) -->
            <inputData id="input2" name="inputB">
                <variable name="inputB" typeRef="number"/>
            </inputData>

            <!-- Input: inputC (nullable) -->
            <inputData id="input3" name="inputC">
                <variable name="inputC" typeRef="number"/>
            </inputData>

            <!-- Decision 1: inputA = inputB -->
            <decision id="decision1" name="CompareStringToNumber">
                <variable name="result" typeRef="boolean"/>
                <informationRequirement>
                    <requiredInput href="#input1"/>
                </informationRequirement>
                <informationRequirement>
                    <requiredInput href="#input2"/>
                </informationRequirement>
                <literalExpression>
                    <text>inputA = inputB</text>
                </literalExpression>
            </decision>

            <!-- Decision 2: null = inputB -->
            <decision id="decision2" name="CompareNullToNumber">
                <variable name="result" typeRef="boolean"/>
                <informationRequirement>
                    <requiredInput href="#input2"/>
                </informationRequirement>
                <literalExpression>
                    <text>null = inputB</text>
                </literalExpression>
            </decision>

            <!-- Decision 3: inputC = null -->
            <decision id="decision3" name="CompareInputToNull">
                <variable name="result" typeRef="boolean"/>
                <informationRequirement>
                    <requiredInput href="#input3"/>
                </informationRequirement>
                <literalExpression>
                    <text>inputC = null</text>
                </literalExpression>
            </decision>
</definitions>
